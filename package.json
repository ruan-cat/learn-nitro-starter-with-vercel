{
  "name": "nitro-app",
  "private": true,
  "packageManager": "pnpm@10.16.0",
  "scripts": {
    "dev": "nitro dev",
    "build": "pnpm build:vercel",
    "build:vercel": "nitro build --preset=vercel",
    "build:cloudflare": "nitro build --preset=cloudflare_module",
    "prepare": "nitro prepare",
    "preview": "node .output/server/index.mjs",
    "deploy:cloudflare": "npx wrangler deploy .output/server/index.mjs --assets public --compatibility-date 2025-09-13"
  },
  "devDependencies": {
    "@ruan-cat/commitlint-config": "^2.1.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "h3": "^1.15.4",
    "nitropack": "^2.12.4",
    "wrangler": "^4.36.0"
  }
}